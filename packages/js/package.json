{
  "name": "@universal-license/client",
  "version": "0.1.0",
  "type": "module",
  "description": "JavaScript/TypeScript SDK for Universal License Server - Secure license validation, purchase flows, and subscription management",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "dev": "rollup -c rollup.config.js -w",
    "test": "vitest run",
    "test:contract": "vitest run -c vitest.contract.config.ts",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "license",
    "license-management",
    "subscription",
    "saas",
    "licensing",
    "authentication",
    "validation",
    "sdk",
    "client",
    "typescript"
  ],
  "author": "Alvin Panford",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alvincoded/universal-license-sdk",
    "directory": "packages/js"
  },
  "bugs": {
    "url": "https://github.com/alvincoded/universal-license-sdk/issues"
  },
  "homepage": "https://github.com/alvincoded/universal-license-sdk#readme",
  "dependencies": {
    "@universal-license/core": "^0.1.0",
    "axios": "^1.13.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "rimraf": "^6.1.2",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
